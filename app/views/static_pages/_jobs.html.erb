<table class="table table-hover">
  <thead>
  <tr>
    <th>Job</th>
    <th>Company</th>
    <th>Address</th>
    <th>Phone</th>
    <th>Description</th>
    <th colspan="5"></th>
  </tr>
  </thead>

  <tbody>
  <% @jobs.each do |job| %>
      <tr>
        <td><%= job.name %></td>
        <td><%= job.company %></td>
        <td><%= job.address %></td>
        <td><%= job.phone %></td>
        <td><%= job.description %></td>
      </tr>
  <% end %>
  </tbody>
</table>
