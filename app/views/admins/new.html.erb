<div class="page-header">
  <h1>Create User</h1>
</div>

<%= form_for(@user,url: admins_path) do |f| %>

  <div class="form-group">
    <%= f.label :email %> <br>
    <%= f.text_field :email %>
  </div>

  <div class="form-group">
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </div>
  <div class="form-group">
    <%= f.label :user_type %><br>
    <%= f.radio_button :user_type, 'member', checked: true %>
    <%= f.label :user_type_member, 'Member' %><br>
    <%= f.radio_button :user_type, 'employee' %>
    <%= f.label :user_type_employee, 'Employee' %><br>
    <%= f.radio_button :user_type, 'admin' %>
    <%= f.label :user_type_admin, 'Admin' %>
  </div>
  <div class="form-group">
    <%= f.submit %>
  </div>

<% end %>